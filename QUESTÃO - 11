#include<stdio.h>
#include<stdlib.h>

int main() {
  setlocale(LC_ALL, "pt_BR.UTF-8");
//Variaveis
 int i, n;
 
//Algoritmo para conferir se o numero e primo ou não
 scanf("%d", &n);
 if(n<=1){
     printf("Seu numero não é primo\n");
 }else{
 	for(i=2; i < n; i++){
 		if(n%i==0){
 			printf("Seu numero não é primo\n");
		 }
	 }
	 if(i==n){
	 	printf("Seu numero é primo");
	 }
 }
  return 0;
}
